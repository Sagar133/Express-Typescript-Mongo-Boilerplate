{
  "name": "api.arthcoin.com",
  "version": "1.0.2",
  "author": "Vassilis Mastorostergios <vmasto@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "test": "jest --watch",
    "lint": "esw -w src test"
  },
  "keywords": [
    "express",
    "babel",
    "boilerplate",
    "scaffold",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "jest",
    "eslint"
  ],
  "dependencies": {
    "@openzeppelin/contracts": "^4.6.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@truffle/hdwallet-provider": "^2.0.8",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^27.5.2",
    "@types/morgan": "^1.9.3",
    "@types/nconf": "^0.10.2",
    "@types/node": "^17.0.41",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.0",
    "ethers": "^5.6.8",
    "mongoose": "^6.3.6",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "^1.10.0",
    "nconf": "^0.12.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "truffle-plugin-verify": "^0.5.26",
    "typescript": "^4.7.3",
    "web-vitals": "^2.1.4"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
